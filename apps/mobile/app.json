{
  "expo": {
    "name": "ViralReelsAI",
    "slug": "viralreels-mobile",
    "version": "1.0.0",
    "orientation": "default",
    "scheme": "viralreels",
    "userInterfaceStyle": "dark",
    "icon": "./assets/icon.png",
    "splash": {
      "backgroundColor": "#0a0a0a"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.viralreels.app"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#0a0a0a"
      },
      "package": "com.viralreels.app"
    },
    "plugins": [
      "expo-router",
      "expo-secure-store"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c20d8002-439b-4743-9cbd-e727063e7a6f"
      }
    }
  }
}
